    "vitest": "^4.0.7"
  },
  "dependencies": {
    "@swc/core": "^1.9.2",
    "inferno": "^9.0.4",
    "inferno-vnode-flags": "^9.0.4",
    "swc-plugin-inferno": "^2.9.0"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
